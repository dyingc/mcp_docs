{
  "base_url": "https://docs.n8n.io",
  "pages_crawled": 0,
  "successful_pages": 0,
  "failed_pages": 0,
  "config_used": {
    "crawling": {
      "crawl": false,
      "delay": 0.1,
      "exclude_patterns": [
        "/api/",
        "/search",
        "\\.xml$",
        "/login",
        "/logout"
      ],
      "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; DocCrawler/1.0; +https://example.com/bot)"
      },
      "include_patterns": [],
      "max_pages": -1,
      "num_threads": 20,
      "skip_extensions": [
        ".pdf",
        ".jpg",
        ".png",
        ".gif",
        ".css",
        ".js",
        ".svg",
        ".ico",
        ".json",
        ".xml",
        ".yaml"
      ],
      "timeout": 10
    },
    "output": {
      "dest_github_repo": "git@github.com:dyingc/mcp_docs.git",
      "directory": "docs_output/n8n",
      "frontmatter": {
        "crawled_date": "2025-06-09",
        "source": "n8n-docs"
      },
      "ignore_images": false,
      "ignore_links": false,
      "llms_doc_prefix": "n8n",
      "llms_txt": {
        "description": "Complete n8n documentation for AI assistance",
        "include_urls": true,
        "title": "n8n Documentation"
      },
      "minimal_size": 1024,
      "output_llms_txt": true
    },
    "site": {
      "allowed_domains": [
        "docs.n8n.io"
      ],
      "content_selectors": [
        "main",
        "article",
        ".content",
        ".documentation"
      ],
      "link_selectors": [
        "a[href]"
      ],
      "name": "n8n Documentation",
      "remove_selectors": [
        "nav",
        "header",
        "footer",
        ".navbar",
        ".nav",
        ".sidebar",
        ".menu",
        ".breadcrumb",
        ".pagination",
        "script",
        "style",
        "noscript",
        ".search-box"
      ],
      "start_urls": [],
      "title_selectors": [
        "title",
        "h1",
        ".page-title"
      ],
      "url": "https://docs.n8n.io/"
    }
  }
}