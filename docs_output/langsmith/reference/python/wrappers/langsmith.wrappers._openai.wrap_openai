    _client : C_,
    _*_ ,
    _tracing_extra : [TracingExtra](langsmith.wrappers._openai.TracingExtra.html#langsmith.wrappers._openai.TracingExtra "langsmith.wrappers._openai.TracingExtra") | None = None_,
